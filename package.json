{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Simple express server with sequelize, pg and jwts",
  "main": "index.js",
  "scripts": {
    "start": "env npx nodemon .",
    "dev": "env DELAY=1000 npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "template",
    "express",
    "jwt",
    "sequelize"
  ],
  "author": "Robbert van den Outenaar",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dot-prop": "^5.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "axios": "^0.21.2",
    "nodemon": "^2.0.4"
  }
}
